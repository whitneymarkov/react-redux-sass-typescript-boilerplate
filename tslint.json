{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "max-line-length": [
      true,
      500
    ],
    "no-default-export": true,
    "no-string-literal": false,
    "prefer-for-of": false
  },
  "rulesDirectory": []
}